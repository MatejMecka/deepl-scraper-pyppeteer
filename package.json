{
  "name": "deepl-scraper-pyppeteer",
  "version": "0.1.1",
  "description": "deepl scraper pp",
  "main": "index.js",
  "repository": "git@github.com:ffreemt/deepl-scraper-pyppeteer.git",
  "author": "freemt",
  "license": "MIT",
  "scripts": {
    "pyright": "nodemon -w deepl_scraper_pp -w .venv -e .py -x pyright deepl_scraper_pp",
    "pytest": "nodemon -w tests -w deepl_scraper_pp -e .py -x pytest tests",
    "style": "nodemon -w deepl_scraper_pp -x black tests deepl_scraper_pp && flake8.bat",
    "pylint": "nodemon -w deepl_scraper_pp -e .py -x pylint deepl_scraper_pp",
    "test:deepl_tr": "nodemon -w tests -e .py -x pytest -k deepl_tr tests"
  }
}
